@model List<LeagueManager.Models.Chips>

<link href="~/css/site.css" rel="stylesheet" />
<link rel="stylesheet" type="text/css" href="https://cdn.datatables.net/1.10.25/css/dataTables.bootstrap4.min.css">





<table data-order='[[ 2, "desc" ]]' data-page-length='50' id="playersTable" class="table table-striped table-bordered" style="width:100%">
    <thead>

        <tr style="background-color:#3D195B; color:#FFF">
            <th>
                Name
            </th>
            <th>
                Team
            </th>
            <th>
                Score
            </th>
            <th>
                Bench Score
            </th>
            <th>
                Transfers Made
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
        <tr style="background-color:whitesmoke">
           <td>
               @item.PlayerName
           </td>
           <td>
           @item.Team
           </td>
        </tr>
        }
    </tbody>
 </table>